{
    "$schema": "http://json-schema.org/schema#",
    "properties": {
        "date_born": {
            "type": "string"
        },
        "dob": {
            "type": "string"
        },
        "education": {
            "anyOf": [
                {
                    "type": "null"
                },
                {
                    "type": "string"
                }
            ]
        },
        "email": {
            "format": "email",
            "maxLength": 127,
            "minLength": 6,
            "pattern": "^\\S+@\\S+\\.\\S+$",
            "title": "Email address",
            "type": "string"
        },
        "lang": {
            "anyOf": [
                {
                    "type": "null"
                },
                {
                    "type": "string"
                }
            ]
        },
        "name": {
            "type": "string"
        },
        "party": {
            "type": "string"
        },
        "place_born": {
            "anyOf": [
                {
                    "type": "null"
                },
                {
                    "type": "string"
                }
            ]
        },
        "pp": {
            "type": "string"
        },
        "profession": {
            "anyOf": [
                {
                    "type": "null"
                },
                {
                    "type": "string"
                }
            ]
        },
        "url": {
            "type": "string"
        }
    },
    "required": [
        "date_born",
        "dob",
        "education",
        "email",
        "lang",
        "name",
        "party",
        "place_born",
        "pp",
        "profession",
        "url"
    ],
    "title": "Welcome",
    "type": "object"
}